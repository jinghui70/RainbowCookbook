{
  "title": "Rainbow 开发手册",
  "author": "jinghui70",
  "language": "zh-hans",
  "plugins": [
    "github",
    "tbfed-pagefooter@^0.0.1",
    "search-pro",
    "-search",
    "-lunr",
    "-sharing",
    "donate",
    "anchor-navigation-ex"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/jinghui70/rainbow"
    },
    "theme-default": {
      "showLevel": true
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright © jinghui70",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "donate": {
      "button": "赞助",
      "alipayText": "支付宝",
      "wechatText": "微信",
      "alipay": "https://github.com/jinghui70/RainbowCookbook/blob/master/assets/alipay.png",
      "wechat": "https://github.com/jinghui70/RainbowCookbook/blob/master/assets/wechat.png"
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "float": {
        "floatIcon": "fa fa-anchor"
      }
    }
  }
}
